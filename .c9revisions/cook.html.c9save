{"ts":1367346242472,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\n<html>\n<meta charset=\"UTF-8\">\n  <head>\n    <title>My Cookbook: <%=item.name%></title>\n    <link rel=\"stylesheet\" href=\"/style.css\">\n  </head>\n  <body>\n    <a href=\"/\"><h1>Cook Profile</h1></a>\n    <dl>\n      <dd><%=item.name%></dd>\n      <dt>Recipes</dt>\n      <dd><a href=\"/recipes/<%=item.recipes%>\"><%=item.recipes%></a></dd>\n      <% if (item.image) { %>\n      <dt>Image URL</dt>\n      <dd><img src=\"<%=item.image%>\" alt=\"<%=item.name%>\"></dd>\n      <% } %>\n    </dl>\n    \n    <!-- hide the following div with a link to open it -->\n    <div class=\"control\">\n      <h2>Add Cook</h2>\n      <form method=\"post\" action=\"/cooks/<%=item._id%>\">\n        <input type=\"hidden\" name=\"_method\" value=\"put\">\n        <!-- The line above tells the server to treat this as a PUT request. -->\n        <label>Name: \n          <input name=\"item[name]\" value=\"<%=item.name%>\" type=\"text\" required>\n        </label>\n        <label>Blog: \n          <input name=\"item[Blog]\" value=\"<%item.blog%>\" type=\"text\" required>\n          </input>\n        </label>\n        <label>Image URL:\n          <input name=\"item[image]\" value=\"<%=item.image%>\" type=\"url\" size=\"80\"> \n        </label>\n        <button>Update</button>\n      </form>\n    </div>\n  </body>\n</html>\n"]],"start1":0,"start2":0,"length1":0,"length2":1254}]],"length":1254}
