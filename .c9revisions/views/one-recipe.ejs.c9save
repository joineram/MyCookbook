{"ts":1367444680823,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\n<html>\n  <head>\n    <title>My Cookbook: <%=item.name%></title>\n    <link rel=\"stylesheet\" href=\"/style.css\">\n  </head>\n  <body>\n    <a href=\"/\"><h1>Home</h1></a>\n    <dl>\n      <dt>Recipe Name</dt>\n      <dd><%=item.name%></dd>\n      <dt>Cook</dt>\n      <dd><a href=\"/cooks/<%=item.cookname%>\"><%=item.cookname%></a></dd>\n      <% if (item.image) { %>\n      <dt>Image URL</dt>\n      <dd><img src=\"<%=item.image%>\" alt=\"<%=item.image%>\"></dd>\n      <% } %>\n      <dt>Cook Time</dt>\n      <dd><%=item.cooktime%></dd>\n      <dt>Ingredient</dt>\n      <dd><%=item.name%></dd>\n      \n    </dl>\n    <div class=\"control\">\n      <h2>Update Recipe</h2>\n      <form method=\"post\" action=\"/candidates/<%=item._id%>\">\n        <input type=\"hidden\" name=\"_method\" value=\"put\">\n        <!-- The line above tells the server to treat this as a PUT request. -->\n        <label>Name: \n          <input name=\"item[name]\" value=\"<%=item.name%>\" type=\"text\" required>\n        </label>\n        <label>Party: \n          <select name=\"item[party]\" required>\n            <% related_items.forEach(function(related_item) { %>\n            <option value=\"<%=related_item._id%>\"\n              <% if (related_item._id === item.party) { %>selected<% } %>>\n              <%=related_item.name%>\n            </option>\n            <% }); %>\n          </select>\n        </label>\n        <label>Image URL:\n          <input name=\"item[image]\" value=\"<%=item.image%>\" type=\"url\" size=\"80\"> \n        </label>\n        <button>Update</button>\n      </form>\n    </div>\n  </body>\n</html>\n"]],"start1":0,"start2":0,"length1":0,"length2":1557}]],"length":1557}
